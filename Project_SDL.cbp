<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Project SDL" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/Project SDL" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<ExtraCommands>
					<Add after='cmd /c copy &quot;$(PROJECT_DIR)Tools\SDL\SDL-1.2.15\bin\SDL.dll&quot; &quot;$(PROJECT_DIR)$(TARGET_OUTPUT_DIR)&quot;' />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/Project SDL" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
				<ExtraCommands>
					<Add after='cmd /c copy &quot;$(PROJECT_DIR)Tools\SDL\SDL-1.2.15\bin\SDL.dll&quot; &quot;$(PROJECT_DIR)$(TARGET_OUTPUT_DIR)&quot;' />
				</ExtraCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Weffc++" />
			<Add option="-std=c++11" />
			<Add option="-Wextra" />
			<Add option="-Wall" />
			<Add directory="Tools/SDL/SDL_image-1.2.12/include" />
			<Add directory="Tools/SDL/SDL_mixer-1.2.12/include" />
			<Add directory="Tools/SDL/SDL_net-1.2.8/include" />
			<Add directory="Tools/SDL/SDL_ttf-2.0.11/include" />
			<Add directory="Tools/SDL/SDL-1.2.15/include" />
		</Compiler>
		<Linker>
			<Add option="-lmingw32 -lSDLmain -lSDL" />
			<Add directory="Tools/SDL/SDL_image-1.2.12/lib" />
			<Add directory="Tools/SDL/SDL_mixer-1.2.12/lib" />
			<Add directory="Tools/SDL/SDL_net-1.2.8/lib" />
			<Add directory="Tools/SDL/SDL_ttf-2.0.11/lib" />
			<Add directory="Tools/SDL/SDL-1.2.15/lib" />
		</Linker>
		<Unit filename="Src/main.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
